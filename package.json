{
  "name": "metronome-app",
  "version": "0.0.1",
  "description": "a web browser metronome",
  "main": "src/index.js",
  "repository": "https://github.com/clhenrick/metronome-app",
  "author": "Chris Henrick <chrishenrick@gmail.com>",
  "license": "MIT",
  "private": true,
  "jest": {
    "testURL": "http://localhost"
  },
  "scripts": {
    "start": "export NODE_ENV=development && webpack-dev-server --config webpack.config.dev.js",
    "build": "export NODE_ENV=production && npm run clean && webpack -p --progress --colors --config webpack.config.prod.js",
    "clean": "rimraf dist",
    "deploy:gh-pages": "npm run build && . ./deploy_gh_pages.sh",
    "eslint-check": "eslint $(find src -name '*.js' ! -name '*.spec.js')",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "format": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.jsx\" \"tests/**/*.js\""
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "6",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "node-sass": "^4.11.0",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.9.2",
    "prettier-eslint": "^8.7.0",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.2",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "webpack-notifier": "^1.5.0",
    "worker-loader": "^1.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "skeleton-scss": "^2.0.4"
  }
}
